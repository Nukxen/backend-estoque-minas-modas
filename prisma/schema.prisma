generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Storage {
  id        String @id @default(uuid())
  IdProduct String
  quantity  Int
}

model Product {
  id        String  @id @default(uuid())
  numericId Int
  name      String
  value     Decimal @db.Money
}

model SellApoints {
  id       String   @id @default(uuid())
  idItem   Int
  data     DateTime
  quantity Int
}
